<form class="{{cssClass}} flexcol" autocomplete="off">
    {{skill}}
    {{#each equippedItems as |item id|}}
    <div class="equppedItems flexrow flex-group-center">
        <div class='item-name'>
        <div class='item-image'>
        <img
            src='{{item.img}}'
            title='{{item.name}}'
            width='24'
            height='24'
        />
        </div>
        <h4>{{item.name}}</h4>
      </div>
      <input type="checkbox" name="system."
    </div>
    {{/each}}
    <input type="text" name="rollCustomBonus" value="" />
    <button class="confirmRoll">Confirm</button>
</form>